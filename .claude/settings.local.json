{
  "env": {
    "BASH_DEFAULT_TIMEOUT_MS": "120000",
    "BASH_MAX_TIMEOUT_MS": "120000"
  },
  "permissions": {
    "allow": [
      "Bash(pip install:*)",
      "Bash(source:*)",
      "Bash(playwright install)",
      "Bash(python test_neo4j_connection.py:*)",
      "Bash(python:*)",
      "Bash(find:*)",
      "Bash(ls:*)",
      "Bash(rm:*)",
      "WebFetch(domain:docs.anthropic.com)",
      "Bash(claude code --help)",
      "Bash(claude code /cost)",
      "Bash(echo $VIRTUAL_ENV)",
      "Bash(mkdir:*)",
      "Bash(touch:*)",
      "Bash(mv:*)",
      "Bash(tree:*)",
      "Bash(test_venv/bin/python -m pytest -v)",
      "Bash(./test_venv/bin/python -m pytest -v)",
      "Bash(./test_venv/bin/pytest -v)",
      "Bash(grep:*)",
      "Bash(echo $WSLENV)",
      "Bash(env)",
      "Bash(.venvScriptsactivate)",
      "Bash(.uv_venvScriptsactivate)",
      "Bash(powershell:*)",
      "Bash(set PYTHONIOENCODING=utf-8)"
    ],
    "deny": [
      "Read(file_path:*.env)",
      "Read(file_path:.env)"
    ]
  }
}